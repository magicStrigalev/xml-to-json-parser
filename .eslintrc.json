{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["prettier", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/prefer-default-export": 0,
    "class-methods-use-this": 0,
    "no-console": 0,
    "function-paren-newline": 0,
    "implicit-arrow-linebreak": 0,
    "comma-dangle": 0,
    "operator-linebreak": 0
  }
}
